{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "DEX order execution engine with WebSocket status updates",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc",
    "start": "pnpm build && prisma migrate deploy && node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test:db": "tsx tests/integration/database.test.ts",
    "test:dex": "tsx tests/integration/dex-router.test.ts",
    "test:queue": "tsx tests/integration/queue.test.ts",
    "test:orders": "tsx tests/integration/order-service.test.ts",
    "test:websocket": "tsx tests/e2e/realtime-websocket.test.ts",
    "test:api": "tsx tests/e2e/api.test.ts",
    "test:integration": "tsx tests/e2e/full-integration.test.ts",
    "test:all:integration": "tsx tests/integration/database.test.ts && tsx tests/integration/dex-router.test.ts && tsx tests/integration/queue.test.ts && tsx tests/integration/order-service.test.ts",
    "test:all:e2e": "bash scripts/test-e2e.sh",
    "test:all": "pnpm test:all:integration && pnpm test:all:e2e"
  },
  "keywords": [
    "dex",
    "order-execution",
    "websocket",
    "solana"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.12.2",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "@fastify/websocket": "^11.2.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "bullmq": "^5.64.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  }
}
